{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "strictNullChecks": true,
    "types": ["vite/client"],
    "noEmit": true,
    "isolatedModules": true,
    "composite": true,
    "declarationMap": true
  },
  "exclude": ["node_modules", "service-apis"]
}
